{
  "version": 2,
  "builds": [
    {
      "src": "./backend/index.py",
      "use": "@vercel/python"
    },
    {
      "src": ".frontend/build/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/backend/(.*)",
      "dest": "/backend/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/build/$1"
    }
  ]
}
